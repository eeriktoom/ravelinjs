{
  "name": "ravelinjs",
  "version": "0.1.0",
  "description": "Ravelin's Javascript Library.",
  "main": "./dist/ravelin.js",
  "scripts": {
    "build": "webpack",
    "release": "npm run build && npm run release-mkdir && npm run release-rename && npm run release-sri",
    "release-mkdir": "VERSION=$(node -e 'console.log(require(\"./package.json\").version)') && mkdir -p ./releases && mkdir -p ./releases/$VERSION",
    "release-rename": "VERSION=$(node -e 'console.log(require(\"./package.json\").version)') && cp ./dist/ravelin.min.js ./releases/$VERSION/ravelin-$VERSION.min.js && cp ./dist/ravelin-no-encrypt.min.js ./releases/$VERSION/ravelin-no-encrypt-$VERSION.min.js",
    "release-sri": "VERSION=$(node -e 'console.log(require(\"./package.json\").version)') && go run github.com/sHesl/sri -out=./releases/$VERSION/sri.json -hash=all ./releases/$VERSION",
    "test": "mocha -C test/test.js",
    "lint": "jshint src",
    "test-cover": "nyc --reporter=html mocha -C test/test.js",
    "test-debug": "mocha --inspect-brk test/test.js",
    "test-appium": "wdio ./test/appium.conf.js",
    "test-crossbrowser": "wdio ./test/crossbrowser.conf.js",
    "test-browserstack": "PARALLEL=5 LOG_LEVEL=error wdio ./test/wdio.conf.js",
    "test-crossbrowser-e2e": "wdio ./test/crossbrowser-e2e.conf.js",
    "testpage-webpack": "webpack ./test/pages/webpack/index.js -o ./test/pages/webpack/bundle.js",
    "test-crossbrowser-live": "sh -u -c 'cbt_tunnels --username $CROSSBROWSERTESTING_USER --authkey $CROSSBROWSERTESTING_KEY --dir ./test/'",
    "go-serve": "goexec 'http.ListenAndServe(\":8088\", http.FileServer(http.Dir(\".\")))'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unravelin/ravelinjs.git"
  },
  "author": "Ravelin (https://github.com/unravelin) <support@ravelin.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/unravelin/ravelinjs/issues"
  },
  "homepage": "https://developer.ravelin.com/libraries-and-sdks/ravelinjs/",
  "devDependencies": {
    "@wdio/browserstack-service": "6.4.0",
    "@wdio/cli": "6.4.0",
    "@wdio/junit-reporter": "6.4.0",
    "@wdio/local-runner": "6.4.0",
    "@wdio/logger": "6.0.16",
    "@wdio/mocha-framework": "6.4.0",
    "@wdio/spec-reporter": "6.4.0",
    "@wdio/sync": "6.4.0",
    "chai": "4.2.0",
    "circle-wait": "1.0.2",
    "core-js-pure": "3.6.5",
    "express": "4.17.1",
    "jshint": "2.10.2",
    "nyc": "14.1.1",
    "sinon": "7.4.1",
    "terser-webpack-plugin": "2.1.1",
    "val-loader": "2.1.1",
    "wait-port": "0.2.2",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.7",
    "xunit-viewer": "6.3.9"
  },
  "jshintConfig": {
    "unused": true,
    "freeze": true,
    "esversion": 3,
    "futurehostile": true,
    "latedef": false,
    "nonew": true,
    "undef": true,
    "browser": true,
    "globals": {
      "require": true,
      "module": true,
      "JSON": true
    }
  }
}
