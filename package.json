{
  "name": "ravelinjs",
  "version": "1.0.0",
  "description": "Ravelin's Browser SDK",
  "main": "./build/ravelin-core+track+encrypt+promise.js",
  "scripts": {
    "watch": "run-p *:watch *:*:watch",
    "build": "rollup --config",
    "build:watch": "rollup --config --watch --no-watch.clearScreen",
    "lint": "jshint lib",
    "release": "run-s build release-mkdir release-rename release-sri",
    "release-mkdir": "mkdir -p ./releases/$npm_package_version",
    "release-rename": "cp ./dist/ravelin.min.js ./releases/$npm_package_version/ravelin-$npm_package_version.min.js && cp ./dist/ravelin-no-encrypt.min.js ./releases/$npm_package_version/ravelin-no-encrypt-$npm_package_version.min.js",
    "release-sri": "go run github.com/sHesl/sri -out=./releases/$npm_package_version/sri.json -hash=all ./releases/$npm_package_version",
    "test": "run-s test:integration",
    "test:unit": "karma start ./test/karma.conf.js --single-run",
    "test:unit:watch": "karma start ./test/karma.conf.js --auto",
    "test:integration": "wdio ./test/wdio.conf.js",
    "go-serve": "goexec 'http.ListenAndServe(\":8088\", http.FileServer(http.Dir(\".\")))'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unravelin/ravelinjs.git"
  },
  "author": "Ravelin (https://github.com/unravelin) <support@ravelin.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/unravelin/ravelinjs/issues"
  },
  "homepage": "https://developer.ravelin.com/libraries-and-sdks/ravelinjs/",
  "devDependencies": {
    "@rollup/plugin-commonjs": "15.0.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "@wdio/browserstack-service": "6.4.0",
    "@wdio/cli": "6.4.0",
    "@wdio/junit-reporter": "6.4.0",
    "@wdio/local-runner": "6.4.0",
    "@wdio/logger": "6.0.16",
    "@wdio/mocha-framework": "6.4.0",
    "@wdio/spec-reporter": "6.4.0",
    "@wdio/sync": "6.4.0",
    "build-url": "6.0.1",
    "chai": "4.2.0",
    "circle-wait": "1.0.2",
    "cors": "2.8.5",
    "express": "4.17.1",
    "joqular": "2.0.4-b",
    "jshint": "2.10.2",
    "karma": "5.2.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-expect": "1.1.3",
    "karma-mocha": "2.0.1",
    "mingo": "3.0.6",
    "ngrok": "3.3.0",
    "node-fetch": "2.6.1",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "on-finished": "2.3.0",
    "rollup": "2.26.11",
    "rollup-plugin-terser": "7.0.2",
    "serve-index": "1.9.1",
    "sinon": "7.4.1",
    "xunit-viewer": "6.3.9",
    "yaku": "1.0.1"
  }
}
